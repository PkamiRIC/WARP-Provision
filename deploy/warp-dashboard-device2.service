[Unit]
Description=WARP Dashboard Device 2 (Vite)
After=network-online.target
Wants=network-online.target

[Service]
Type=simple
WorkingDirectory=/opt/warp/app/dashboard-device2
ExecStart=/usr/bin/npm run preview -- --host 0.0.0.0 --port 5172
Restart=always
EnvironmentFile=/etc/warp/env
Environment=NODE_ENV=production

[Install]
WantedBy=multi-user.target
